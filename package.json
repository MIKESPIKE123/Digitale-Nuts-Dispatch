{
  "name": "dn-dispatch",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "import:data": "node scripts/import-nuts-data.mjs",
    "qa:known-addresses": "node scripts/validate-known-addresses.mjs",
    "import:impact": "node scripts/import-impact-data.mjs",
    "impact:evaluate": "node scripts/evaluate-impact-priority.mjs",
    "impact:phase4": "npm run import:data && npm run import:impact && npm run impact:evaluate",
    "typecheck": "tsc -b --pretty false",
    "test": "vitest run",
    "test:watch": "vitest",
    "quality": "npm run typecheck && npm run test",
    "dev": "npm run import:data && npm run import:impact && vite --host 0.0.0.0 --port 3012",
    "build": "npm run import:data && npm run import:impact && tsc -b && vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3012"
  },
  "dependencies": {
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "maplibre-gl": "^5.9.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-map-gl": "^8.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^4.7.0",
    "typescript": "^5.9.3",
    "vite": "^6.3.6",
    "vitest": "^2.1.8",
    "xlsx": "^0.18.5"
  }
}
